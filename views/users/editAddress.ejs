<%- include('userSubHeader') %>

<div class="container">
    <div class="card-body">
        <form method="post" name="enq" action="/editAddress/<%= address._id %>">
            <div class="row">
                <div class="form-group col-md-6">
                    <label>House Name <span class="required">*</span></label>
                    <input required="" class="form-control square" name="houseNo" type="text" value="<%- address.houseNo %>">
                </div>
                <div class="form-group col-md-6">
                    <label>Street <span class="required">*</span></label>
                    <input required="" class="form-control square" name="street" value="<%- address.street %>">
                </div>
                <div class="form-group col-md-12">
                    <label>Landmark</label>
                    <input class="form-control square" name="landmark" type="text" value="<%- address.landmark%>">
                </div>
                <div class="form-group col-md-12">
                    <label>Pincode <span class="required">*</span></label>
                    <input required="" class="form-control square" name="pincode" type="text" value="<%- address.pincode %>">
                </div>
                <div class="form-group col-md-12">
                    <label>City <span class="required">*</span></label>
                    <input required="" class="form-control square" name="city" type="text" value="<%- address.city %>">
                </div>
                <div class="form-group col-md-12">
                    <label>District</label>
                    <input class="form-control square" name="district" type="text" value="<%- address.district %>">
                </div>
                <div class="form-group col-md-12">
                    <label>State <span class="required">*</span></label>
                    <input required="" class="form-control square" name="state" type="text" value="<%- address.state %>">
                </div>
                <div class="form-group col-md-12">
                    <label>Country <span class="required">*</span></label>
                    <input required="" class="form-control square" name="country" type="text" value="<%- address.country %>">
                </div>
                <div class="form-group col-md-12">
                  <label>Phone Number <span class="required">*</span></label>
                  <input required="" class="form-control square" name="phoneNumber" type="text"  value="<%- address.phoneNumber %>">
              </div>
                <div class="form-group col-md-12">
                    <label>Address Type <span class="required">*</span></label>
                    <select class="form-control square" name="addressType" required>
                        <option value="home">Home</option>
                        <option value="work">Work place</option>
                        <option value="temp">Other</option>
                    </select>
                </div>
                <div class="col-md-12">
                    <button type="submit" class="btn btn-fill-out submit" name="submit" value="Submit">Save</button>
                </div>
            </div>
        </form>
    </div>
</div>

  <!-- Vendor JS-->
